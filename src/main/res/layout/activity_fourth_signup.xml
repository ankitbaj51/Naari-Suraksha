<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kakaninavin.womensafety.fourth_signup">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="26dp"
        android:text="Name"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_above="@+id/textView4"
        android:layout_alignStart="@+id/textView"
        android:text="Mobile_no"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="74dp"
        android:layout_height="45dp"
        android:layout_above="@+id/textView5"
        android:layout_alignStart="@+id/textView"
        android:text="Address"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="76dp"
        android:layout_height="44dp"
        android:layout_above="@+id/textView6"
        android:layout_alignStart="@+id/textView"
        android:layout_marginBottom="-206dp"
        android:text="Email_id"
        android:textColor="#000000" />

    <EditText
        android:id="@+id/TEname"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="21dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/btnmove4"
        android:layout_width="176dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="109dp"
        android:background="@android:color/holo_red_dark"
        android:onClick="save"
        android:text="SIGNUP"
        android:textColor="#050404" />

    <Button
        android:id="@+id/btnMove5"
        android:layout_width="177dp"
        android:layout_height="67dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="27dp"
        android:background="@android:color/holo_red_dark"
        android:text="BACK"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="72dp"
        android:layout_height="41dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView"
        android:layout_marginTop="206dp"
        android:text="Password"
        android:textColor="#000000" />

    <EditText
        android:id="@+id/TEmail"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="21dp"
        android:ems="10"
        android:inputType="textEmailAddress" />

    <EditText
        android:id="@+id/TEpass"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="21dp"
        android:ems="10"
        android:inputType="textPassword"
        android:password="true" />

    <EditText
        android:id="@+id/TEmob"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="21dp"
        android:layout_marginTop="54dp"
        android:ems="10"
        android:inputType="phone"
        android:phoneNumber="true" />

    <EditText
        android:id="@+id/TEAddress"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="21dp"
        android:ems="10"
        android:inputType="textPostalAddress" />

    <TextView
        android:id="@+id/txtree"
        android:layout_width="70dp"
        android:layout_height="36dp"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:text="Confirm Password"
        android:textColor="#000000" />

    <EditText
        android:id="@+id/TEconpass"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView6"
        android:layout_marginEnd="21dp"
        android:ems="10"
        android:inputType="textPassword"
        android:password="true" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView"
        android:layout_toEndOf="@+id/textView"
        android:text="*"
        android:textColor="@android:color/holo_red_dark" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_toEndOf="@+id/textView2"
        android:text="*"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="14sp" />

</RelativeLayout>